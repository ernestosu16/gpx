{{ form_start(form) }}
{# @var fv \Symfony\Component\Form\FormView #}
{% for fv in form %}
    {% if fv.vars.name != '_token' %}
        <div class="form-group">
            {{ form_label(fv) }}
            <div class="col-sm-10">
                {{ form_widget(fv) }}
                {{ form_help(fv) }}
                {{ form_errors(fv) }}
            </div>
        </div>
    {% endif %}
{% endfor %}
<button class="btn btn-sm btn-success">{{ button_label|default('save'|trans([],'nomenclador')) }}</button>

<a href="{{ path(config.routes.index) }}"
   class="btn btn-sm btn-default">{{ 'back to list'|trans([],'nomenclador') }}
</a>

{{ form_end(form) }}
