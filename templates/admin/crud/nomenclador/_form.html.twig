{{ form_start(form) }}
{# @var fv \Symfony\Component\Form\FormView #}
{% for fv in form %}
    {% if fv.vars.name != '_token' || fv.vars.name != 'parametros' %}
        <div class="form-group">
            {{ form_label(fv) }}
            <div class="col-sm-10">
                {{ form_widget(fv) }}
                {{ form_help(fv) }}
                {{ form_errors(fv) }}
            </div>
        </div>
    {% endif %}
{% endfor %}
{#{% for parametro in form.parametros %}#}
{#    {{ form(parametro) }}#}
{#{% endfor %}#}

<button class="btn btn-sm btn-success">{{ button_label|default('guardar'|trans([],'admin')) }}</button>

<a href="{{ path(config.routes.index) }}"
   class="btn btn-sm btn-default">{{ 'No se encontraron registros'|trans([],'admin') }}
</a>

{{ form_end(form) }}
