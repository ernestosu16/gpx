{{ form_start(form) }}
{{ form_errors(form) }}
{% for fv in form %}
    {% if fv.vars.name != '_token' %}
        <div class="form-group">
            {{ form_label(fv) }}
            <div class="col-sm-10">
                {{ form_widget(fv) }}
                {{ form_help(fv) }}
                {{ form_errors(fv) }}
            </div>
        </div>
    {% endif %}
{% endfor %}
<button class="btn btn-sm btn-success">{{ button_label|default('guardar'|trans([],'admin')) }}</button>

<a href="{{ path(settings.routes.index) }}" class="btn btn-sm btn-default">
    {{ 'volver a la lista'|trans([],'admin') }}
</a>
{{ form_end(form) }}
