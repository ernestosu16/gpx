{{ form_start(form) }}
{# @var fv \Symfony\Component\Form\FormView #}
{% for fv in form %}
    {% if fv.vars.name != '_token' and fv.vars.name != 'parametros' %}
        <div class="form-group">
            {{ form_label(fv) }}
            <div class="col-sm-10">
                {{ form_widget(fv) }}
                {{ form_help(fv) }}
                {{ form_errors(fv) }}
            </div>
        </div>
    {% endif %}
{% endfor %}
{% if form.parametros is defined %}
    {% for parametro in form.parametros %}
        <div class="form-group">
            {{ form_label(parametro) }}
            <div class="col-sm-10">
                {{ form_widget(parametro) }}
                {{ form_help(parametro) }}
                {{ form_errors(parametro) }}
            </div>
        </div>
    {% endfor %}
{% endif %}
<button class="btn btn-sm btn-success">{{ button_label|default('guardar'|trans([],'admin')) }}</button>

<a class="btn btn-sm btn-default" href="{{ path(config.routes.index,{'parent': nomencladorParent.codigo}) }}">
    {{ 'volver a la lista'|trans([],'admin') }}
</a>
{{ form_errors(form) }}
{{ form_end(form) }}
