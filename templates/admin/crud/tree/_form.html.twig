{{ form_start(form) }}
{# @var fv \Symfony\Component\Form\FormView #}
{% for fv in form %}
    {% if fv.vars.name != '_token' %}
        <div class="form-group">
            {{ form_label(fv) }}
            <div class="col-sm-10">
                {{ form_widget(fv) }}
                {{ form_help(fv) }}
                {{ form_errors(fv) }}
            </div>
        </div>
    {% endif %}
{% endfor %}
<button class="btn btn-sm btn-success">{{ button_label|default('save'|trans([],'nomenclador')) }}</button>

<a class="btn btn-sm btn-default" href="{{ path(config.routes.index,{'parent': nomencladorParent.codigo}) }}">
    {{ 'back to list'|trans([],'nomenclador') }}
</a>
{{ form_errors(form) }}
{{ form_end(form) }}
